{
    "name": "Bitter for Chrome",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Remove posts beyond a certain time that do not have any replies",
    "content_scripts": [
        {
            "matches": [ "http://alpha.app.net/*", "https://alpha.app.net/*" ],
            "js":      [ "jquery-1.8.0.min.js", "bitter.js" ]
        }
    ],
    "permissions": [
        "http://alpha.app.net/*",
        "https://alpha.app.net/*"
    ]
        
}